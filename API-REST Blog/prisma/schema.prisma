generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Categorie {
  id Int @id @default(autoincrement())
  name String @unique
  movies Movie[]
}

model Movie {
    Id Int @id @default(autoincrement()) 
    title String @unique
    year  String
    time  String
    categorie Categorie @relation(fields: [categorieID], references: [id])
    categorieID Int
}
