<%- include('../partials/header.ejs')%>

<body>
    <%- include('../partials/navbar.ejs')%>

    <div class="asks__container">
        <h1>Perguntas</h1>

        <a href="/ask" class="btn btn-primary">Fazer uma Pergunta</a>
        <hr>
        <% ask.forEach(ask => {%>
            <div class="card">
                <div class="card-body">
                    <h3><%=ask.title%></h3>
                </div>
                <div class="card-footer">
                    <button class="btn btn-primary">Responder</button>
                </div>
            </div>
        <%})%>

    </div>
</body>


 
<%- include('../partials/footer.ejs')%>     